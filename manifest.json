{
  "name": "Show Random Bookmark",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "*://*/*",
    "bookmarks"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ],
  "icons": { "48": "icon.png"},
  "manifest_version": 2
}